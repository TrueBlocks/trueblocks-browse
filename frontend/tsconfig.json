{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "@components": [
        "./components/index"
      ],
      "@editors": [
        "./editors/index"
      ],
      "@hooks": [
        "./hooks/index"
      ],
      "@layout": [
        "./layout/index"
      ],
      "@state": [
        "./state/index"
      ],
      "@views": [
        "./views/index"
      ],
      "@gocode/*": [
        "../wailsjs/go/*"
      ],
      "@runtime": [
        "../wailsjs/runtime/runtime"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
